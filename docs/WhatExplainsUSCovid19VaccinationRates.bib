@article{hallCOVID19VaccineCoverage2021,
  title = {{{COVID-19}} Vaccine Coverage in Health-Care Workers in {{England}} and Effectiveness of {{BNT162b2 mRNA}} Vaccine against Infection ({{SIREN}}): A Prospective, Multicentre, Cohort Study},
  shorttitle = {{{COVID-19}} Vaccine Coverage in Health-Care Workers in {{England}} and Effectiveness of {{BNT162b2 mRNA}} Vaccine against Infection ({{SIREN}})},
